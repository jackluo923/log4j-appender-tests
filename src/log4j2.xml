<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="com.yscope.test.log4j">
    <Appenders>
        <CLPAppenderImpl name="CompressedLogAppender">
            <PatternLayout pattern=" %-5level - %msg%n"/>
<!--            <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level - %msg%n"/>-->
        </CLPAppenderImpl>
    </Appenders>
    <Loggers>
        <Root>
            <AppenderRef ref="CompressedLogAppender" level="debug"/>
        </Root>
    </Loggers>
</Configuration>